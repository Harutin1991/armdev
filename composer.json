{
	"name": "armdev/framewor",
	"description": "Arm Dev Custom Framework.",
	"keywords": [
		"framework",
		"laravel"
	],
	"type": "project",
	"require": {
		"php": ">=5.6"
	},
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
	"autoload": {
		"classmap": [
		  "src/"
		],
		"psr-4": {
		  "App\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
		  "Tests\\": "tests/"
		},
		"files": [
		  "tests/helpers.php"
		]
	}
}
